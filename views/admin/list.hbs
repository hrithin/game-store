<div style="margin-top:70px ;">
    <ul class="nav nav-pills mb-3 m-4" id="ex1" role="tablist">
        <li class="nav-item" role="presentation">
            <a class="nav-link" id="ex1-tab-1" data-mdb-toggle="pill" href="/admin/games" role="tab">Add Games</a>
        </li>
        <li class="nav-item" role="presentation">
            <a class="nav-link active" id="ex1-tab-2" data-mdb-toggle="pill" href="#ex1-pills-2" role="tab">Games
                List</a>
    </ul>
    <!-- Pills navs -->

    <!-- Pills content -->
    <div class="tab-content" id="ex1-content">
        <div class="tab-pane fade show active" id="ex1-pills-1" role="tabpanel" aria-labelledby="ex1-tab-1">
            <div class="container">
                <table class="table table-striped table-hover table-bordered ">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Name</th>
                            <th scope="col">Catogery</th>
                            <th scope="col">Description</th>
                            <th scope="col">Price</th>
                            <th scope="col">Image</th>
                            <th scope="col">File name</th>
                            <th scope="col">actions</th>


                        </tr>
                    </thead>
                    {{#each pro}}
                    <tbody>
                        <tr>
                            <th scope="row">{{@index}}</th>
                            <td>{{this.gname}}</td>
                            <td>{{this.gcat}}</td>
                            <td style="max-width: 150px;">{{this.gdis}}</td>
                            <td>{{this.gprice}}</td>     
                            <td><img class="card-img" style="width: 60px ;"
                        src="/images/{{this.gimage}}"></td>  
                            <td>{{this.gfile}}</td> 
                            <td style=""><a class="btn btn-primary" href="/admin/list/delete/l{{this._id}}" role="button">Delete</a></td> 

                        </tr>

                    </tbody>
                 {{/each}}
                </table>

            </div>

        </div>
        <div class="tab-pane fade" id="ex1-pills-2" role="tabpanel" aria-labelledby="ex1-tab-2">
            Tab 2 content
        </div>

    </div>

</div>